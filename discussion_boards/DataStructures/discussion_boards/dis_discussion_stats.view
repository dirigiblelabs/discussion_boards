{
  "viewName":"DIS_DISCUSSION_STATS",
  "query":"SELECT
	MAX(DISC_PUBLISH_DATE) AS LATEST_PUBLISH_DATE,
	COUNT(DISC_REPLY_TO_DISC_ID) AS REPLIES,
	COUNT(DISTINCT DISC_USER) AS PARTICIPANTS,
	DISB_ID	
FROM DIS_COMMENT, DIS_BOARD
WHERE DISC_DISB_ID = DISB_ID AND 
	 DISC_REPLY_TO_DISC_ID > -1
GROUP BY DISB_ID
ORDER BY LATEST_PUBLISH_DATE, REPLIES, PARTICIPANTS DESC"
}
